{
  "name": "koci-blog",
  "description": "Personal Tech Blog",
  "version": "1.0.1",
  "author": "Alessio Koci",
  "repository": "git@github.com:alewin/koci-blog.git",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "check": "npx npm-check-updates -u"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "eslint-utils": "^2.0.0",
    "gatsby": "^2.20.18",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-catch-links": "^2.2.1",
    "gatsby-plugin-feed": "^2.4.1",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-nprogress": "^2.2.1",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "3.2.2",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "2.5.4",
    "gatsby-plugin-sitemap": "2.3.1",
    "gatsby-plugin-typography": "^2.4.1",
    "gatsby-remark-autolink-headers": "^2.2.1",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "3.2.2",
    "gatsby-remark-prismjs": "3.4.1",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "2.7.1",
    "gatsby-transformer-sharp": "2.4.4",
    "lodash": "^4.17.15",
    "lodash.kebabcase": "^4.1.1",
    "md5": "^2.2.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "prismjs": "^1.20.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-github-btn": "^1.1.1",
    "react-helmet": "^6.0.0",
    "react-typography": "^0.16.19",
    "sharp": "^0.25.2",
    "typography": "^0.16.19",
    "url-join": "^4.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cli-glob": "^0.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.0.4",
    "remark-cli": "^8.0.0",
    "remark-preset-lint-recommended": "^4.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.3.1",
    "stylelint-config-standard": "^20.0.0",
    "write-good": "^1.0.2"
  },
  "keywords": [
    "alessio koci",
    "javscript",
    "dev",
    "react",
    "frontend",
    "tech",
    "articles",
    "blog"
  ],
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  },
  "license": "MIT"
}
